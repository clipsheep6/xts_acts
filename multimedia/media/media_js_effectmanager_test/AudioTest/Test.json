{
    "description": "Configuration for audio manager Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "150000000",
        "testcase-timeout": "30000000",
        "bundle-name": "ohos.acts.multimedia.audio.audiotest",
        "package-name": "ohos.acts.multimedia.audio.audiotest",
        "shell-timeout": "6000000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAudioTestJsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "hilog -Q pidoff",
                "power-shell setmode 602"
            ],
            "teardown-command":[
                "power-shell setmode 600"
            ],
            "cleanup-apps": true
        },
        {
            "type": "PushKit",
            "pre-push": [],
            "push": [
            ]
        }
    ]
}