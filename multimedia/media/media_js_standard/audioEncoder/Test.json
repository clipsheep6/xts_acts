{
    "description": "Configuration for audioEncoder Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "2000000",
        "package": "ohos.acts.multimedia.audio.audioencoder",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAudioEncoderJsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "rm -R /storage/media/100/local/files/Audios",
                "mkdir /storage/media/100/local/files/AudioEncode",
                "chmod -R 777 /storage/media/100/local/files/*"
            ],
            "teardown-command":[
            ]
        },
        {
            "type": "PushKit",
            "pre-push": [
            ],
            "push": [
                "./resource/audio/audioEncode/S16LE.pcm ->/storage/media/100/local/files/AudioEncode"
            ]
        },
        {
            "type": "ShellKit",
            "run-command": [
                "chmod -R 777 /storage/media/100/local/files/AudioEncode",
                "chmod  777 /storage/media/100/local/files/AudioEncode/*",
                "killall com.ohos.medialibrary.MediaScannerAbilityA",
                "aa start  -a MediaScannerAbility -b com.ohos.medialibrary.MediaScannerAbilityA"
            ],
            "teardown-command":[
                "rm -rf /storage/media/100/local/files/*"
            ]
        }
    ]
}