{
    "description": "Configuration for native buffer Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "shell-timeout": "600000",
        "bundle-name": "com.example.avscreencapturendkxts",
        "module-name": "entry_test"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsScreenCaptureNdkTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}