{
    "description": "Configuration for videoplayer Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "2000000",
        "package": "ohos.acts.multimedia.video.videoplayer",
		"shell-timeout": "200000"
    },
    "kits": [
        {
          "test-file-name": [
            "ActsVideoPlayerDatasrcJsTest.hap"
          ],
          "type": "AppInstallKit",
          "cleanup-apps": true
        },
        {
          "type": "ShellKit",
          "run-command": [
            "mkdir -p /data/app/el1/bundle/public/ohos.acts.multimedia.video.videoplayer/ohos.acts.multimedia.video.videoplayer/assets/entry/resources/rawfile",
			"mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.video.videoplayer/haps/entry/files",
			"chmod -R 777 /data/app/el2/100/base/ohos.acts.multimedia.video.videoplayer/haps/entry/files"
          ],
          "teardown-command":[
          ]
        },
        {
          "type": "PushKit",
          "pre-push": [
          ],
          "push": [
            "./resource/audio/H264_AAC.mp4 ->/data/media/",
			"./resource/audio/H264_AAC.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.video.videoplayer/haps/entry/files/",
            "./resource/format/h263_mp3.ts ->/data/app/el2/100/base/ohos.acts.multimedia.video.videoplayer/haps/entry/files/"
          ]
        },
	    {
          "type": "ShellKit",
          "run-command": [
			"chmod -R 777 /data/app/el2/100/base/ohos.acts.multimedia.video.videoplayer/haps/entry/files/*"
          ],
          "teardown-command":[
          ]
        }
    ]
}
