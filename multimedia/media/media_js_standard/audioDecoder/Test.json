{
    "description": "Configuration for audioDecoder Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "2000000",
        "package": "ohos.acts.multimedia.audio.audiodecoder",
		"shell-timeout": "60000"
    },
    "kits": [
        {
            "type": "PushKit",
            "pre-push": [
            ],
            "push": [
            ]
        },
		{
            "type": "ShellKit",
            "run-command": [
                "chmod -R 777 /data/media/*",
                "setenforce 0"
            ],
            "teardown-command":[
                "setenforce 1"
            ]
        },
        {
            "test-file-name": [
                "AudioDecoderJsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}
