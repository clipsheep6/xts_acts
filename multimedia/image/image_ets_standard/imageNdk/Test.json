{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "300000",
        "bundle-name": "ohos.acts.multimedia.image.imageNdk",
        "module-name": "entry",
        "shell-timeout": "300000",
        "testcase-timeout": "180000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsImageNdkTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files/"
            ]
        },
        {
            "type": "PushKit",
            "pre-push": [],
            "push": [
                "./resource/image/docx.ico ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/SVG.svg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/test.arw ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/test.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/test.webp ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/test_exif_v2.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/test5.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/test6.gif ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/test7.png ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files",
                "./resource/image/test8.bmp ->/data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files"
            ]
        },
        {
            "type": "ShellKit",
            "run-command": [
                "chmod -R 777 /data/app/el2/100/base/ohos.acts.multimedia.image.imageNdk/haps/entry/files/*"
            ]
        }
    ]
}

