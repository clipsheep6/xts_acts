{
    "description": "Configuration for userfilemgr Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "300000",
        "package": "ohos.acts.multimedia.mediaLibrary",
        "shell-timeout": "600000"
    },
    "kits": [
        {
            "type": "ShellKit",
            "pre-push": [
            ],
            "run-command": [
                "rm -rf /storage/media/100/local/files/*",
                "rm -rf /data/app/el2/100/database/com.ohos.medialibrary.medialibrarydata/*",
                "kill -9 `pidof com.ohos.medialibrary.medialibrarydata`",
                "scanner",
                "sleep 5",
                "mkdir -pv /storage/media/100/local/files/{Camera,Pictures,Videos,Audios,Documents}",
                "mkdir /storage/media/100/local/temp",
                "param set persist.ace.testmode.enabled 1"
            ]
        },
        {
            "type": "PushKit",
            "pre-push": [
            ],
            "push": [
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/01.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/01.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/commitModifyCallback01.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/commitModifyCallback01.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/commitModifyPromise01.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/commitModifyPromise01.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/favCallback01.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/favCallback01.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/favCallback02.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/favCallback02.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/favCallback03.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/favCallback03.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/favPromise01.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/favPromise01.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/favPromise02.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/favPromise02.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/favPromise03.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/favPromise03.mp4",

                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/thumCallback01.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/thumCallback01.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/thumCallback02.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/thumCallback02.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/thumPromise01.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/thumPromise01.mp4",
                "./resource/medialibrary/01.jpg ->/storage/media/100/local/temp/thumPromise02.jpg",
                "./resource/medialibrary/01.mp4 ->/storage/media/100/local/temp/thumPromise02.mp4"
            ]
        },
        {
            "type": "ShellKit",
            "run-command": [
                "mediatool send /storage/media/100/local/temp/01.jpg",
                "mediatool send /storage/media/100/local/temp/01.mp4",
                "mediatool send /storage/media/100/local/temp/commitModifyCallback01.jpg",
                "mediatool send /storage/media/100/local/temp/commitModifyCallback01.mp4",
                "mediatool send /storage/media/100/local/temp/commitModifyPromise01.jpg",
                "mediatool send /storage/media/100/local/temp/commitModifyPromise01.mp4",
                "mediatool send /storage/media/100/local/temp/favCallback01.jpg",
                "mediatool send /storage/media/100/local/temp/favCallback01.mp4",
                "mediatool send /storage/media/100/local/temp/favCallback02.jpg",
                "mediatool send /storage/media/100/local/temp/favCallback02.mp4",
                "mediatool send /storage/media/100/local/temp/favCallback03.jpg",
                "mediatool send /storage/media/100/local/temp/favCallback03.mp4",
                "mediatool send /storage/media/100/local/temp/favPromise01.jpg",
                "mediatool send /storage/media/100/local/temp/favPromise01.mp4",
                "mediatool send /storage/media/100/local/temp/favPromise02.jpg",
                "mediatool send /storage/media/100/local/temp/favPromise02.mp4",
                "mediatool send /storage/media/100/local/temp/favPromise03.jpg",
                "mediatool send /storage/media/100/local/temp/favPromise03.mp4",
                "mediatool send /storage/media/100/local/temp/thumCallback01.jpg",
                "mediatool send /storage/media/100/local/temp/thumCallback01.mp4",
                "mediatool send /storage/media/100/local/temp/thumCallback02.jpg",
                "mediatool send /storage/media/100/local/temp/thumCallback02.mp4",
                "mediatool send /storage/media/100/local/temp/thumPromise01.jpg",
                "mediatool send /storage/media/100/local/temp/thumPromise01.mp4",
                "mediatool send /storage/media/100/local/temp/thumPromise02.jpg",
                "mediatool send /storage/media/100/local/temp/thumPromise02.mp4",
                
                "chmod -R 777 /storage/media/100/local/files/*",
                "chmod -R 777 /data/service/el2/100/hmdfs/account/files/*",
                "hilog -Q pidoff",
                "hilog -p off",
                "hilog -b I",
                "hilog -b D -D 0xD002B70", 
                "scanner",
                "sleep 10"
            ]
        },
        {
            "test-file-name": [
                "ActsUserFileMgrFileAsset.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}