{
    "description": "Configuration for OhAudioNdk Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "shell-timeout": "600000",
        "bundle-name": "com.example.ohaudiondk",
        "module-name": "com.example.ohaudiondk"
    },
    "kits": [
        {
            "test-file-name": [
                "OhAudioNdkMainTest.hap",
                "OhCapturerTest.hap",
                "OhRendererTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}
