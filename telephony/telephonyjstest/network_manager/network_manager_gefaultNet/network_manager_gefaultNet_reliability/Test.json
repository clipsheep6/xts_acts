{
    "description": "Configuration for NetworkManager Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "1800",
        "package": "com.ohos.networkmanagertest",
        "shell-timeout": "1800",
        "native-test-timeout":"1800"
    },
    "kits": [
        {
            "test-file-name": [
                "$module.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}
