{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "360000",
        "bundle-name": "com.netconn.netconnndktest",
        "module-name": "com.netconn.napitest",
        "shell-timeout": "360000",
        "testcase-timeout": 70000
    },
    "kits": [{
        "test-file-name": [
            "ActsNetConnTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602"
        ]
    }]
}