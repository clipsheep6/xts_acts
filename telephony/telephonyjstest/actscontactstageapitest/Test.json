{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1800000",
        "bundle-name": "com.ohos.actscontactstageapitest",
        "package-name": "com.ohos.actscontactstageapitest",
        "shell-timeout": "1800000",
        "testcase-timeout": 70000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsContactStageAPITest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "uinput -T -m 200 1000 200 200 200",
                "param set persist.ace.testmode.enabled 1"
            ]
        }
    ]
}
