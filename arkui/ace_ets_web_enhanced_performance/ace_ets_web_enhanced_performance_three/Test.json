{
    "description": "Configuration for ActsAceWebPerformanceTestThree Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.aceweb.performance3.test",
        "module-name": "com.aceweb.performance3.test",
        "shell-timeout": "600000",
        "testcase-timeout": 60000
    },
    "kits": [{
        "test-file-name": [
            "ActsAceWebPerformanceTestThree.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602",
            "cd ./storage/media/100/local/files/Documents/",
            "touch 1.json"
        ]
    }]
}