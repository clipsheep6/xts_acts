{
    "description": "Configuration for ActsAceWebEnhanced Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.acewebenhanced.test3",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 60000
    },
    "kits": [{
        "test-file-name": [
            "ActsAceWebEnhancedTestThree.hap",
            "ActsAceWebEnhancedTestThreeMain.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602",
            "power-shell timeout -o 864000000",
            "cd ./storage/media/100/local/files/Documents/",
            "touch 1.json"
        ]
    }]
}