{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "600000",
    "bundle-name": "com.example.actsnotificationcancelasbundletest",
    "module-name": "entry_test",
    "shell-timeout": "600000",
    "testcase-timeout": 10000
  },
  "kits": [
    {
      "test-file-name": [
        "ActsNotificationCancelRely.hap",
        "ActsNotificationCancelTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "wait 10",
        "power-shell wakeup",
        "power-shell setmode 602"
      ]
    }
  ]
}