            ],
            "type": "PushKit"
        },
        {
            "type": "ShellKit",
            "run-command": [
                "remount",
                "mkdir /data/test",
                "cd /data/local/tmp"
            ],
            "teardown-localcommand": [
                "python testcases/vulkandata/genlocal.py"
            ]
        }
    ]
}