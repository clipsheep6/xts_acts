                "vulkandata/vk-default/imageless-framebuffer.txt->/data/local/tmp/imageless-framebuffer.txt",
                "vulkandata/vk-default/info.txt->/data/local/tmp/info.txt",
                "vulkandata/vk-default/master.txt->/data/local/tmp/master.txt",
                "vulkandata/vk-default/memory-model.txt->/data/local/tmp/memory-model.txt",
                "vulkandata/vk-default/memory.txt->/data/local/tmp/memory.txt",
                "vulkandata/vk-default/mesh-shader.txt->/data/local/tmp/mesh-shader.txt",
