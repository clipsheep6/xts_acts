{
    "description": "Config for ActsVulkanTest test cases",
    "driver": {
        "module-name": "ActsVulkanTests",
        "native-test-timeout": "120000",
        "native-test-device-path": "/data/local/tmp",
        "runtime-hint": "100s",
        "type": "CppTest"
    },
    "kits": [
        {
            "pre-push" : [
                "mkdir -p /data/local/tmp/test/fixtures"
            ],
            "post-push" : [
                "mount -o rw,remount /",
                "chmod -R 777 /data/local/tmp/*"
            ],
            "push": [
                "ActsVulkanTests->/data/local/tmp/ActsVulkanTests",
                "vulkandata/glcts->/data/local/tmp/glcts",
                "vulkandata/libdeqp_ohos_platform.z.so->/system/lib/libdeqp_ohos_platform.z.so",
                "vulkandata/librosen_context.z.so->/system/lib/librosen_context.z.so",
                "vulkandata/libdeqp_spirv.z.so->/system/lib/libdeqp_spirv.z.so",
                "vulkandata/libdeqp_spvremapper.z.so->/system/lib/libdeqp_spvremapper.z.so",     
                "vulkandata/vk-default/api.txt->/data/local/tmp/api.txt",
                "vulkandata/vk-default/binding-model.txt->/data/local/tmp/binding-model.txt",
                "vulkandata/vk-default/clipping.txt->/data/local/tmp/clipping.txt",
                "vulkandata/vk-default/compute.txt->/data/local/tmp/compute.txt",
                "vulkandata/vk-default/conditional-rendering.txt->/data/local/tmp/conditional-rendering.txt",
                "vulkandata/vk-default/descriptor-indexing.txt->/data/local/tmpdescriptor-indexing.txt",
                "vulkandata/vk-default/device-group.txt->/data/local/tmp/device-group.txt",
                "vulkandata/vk-default/draw.txt->/data/local/tmp/draw.txt",
                "vulkandata/vk-default/drm-format-modifiers.txt->/data/local/tmp/drm-format-modifiers.txt",
                "vulkandata/vk-default/dynamic-rendering.txt->/data/local/tmp/dynamic-rendering.txt",
                "vulkandata/vk-default/dynamic-state.txt->/data/local/tmp/dynamic-state.txt",
                "vulkandata/vk-default/fraction-mandatory-tests.txt->/data/local/tmp/fraction-mandatory-tests.txt",
                "vulkandata/vk-default/fragment-operations.txt->/data/local/tmp/fragment-operations.txt",
                "vulkandata/vk-default/fragment-shader-interlock.txt->/data/local/tmp/fragment-shader-interlock.txt",
                "vulkandata/vk-default/fragment-shading-rate.txt->/data/local/tmp/fragment-shading-rate.txt",
