{
    "app":{
        "bundleName": "com.ohos.faceauth.service.appAuth",
        "vendor": "ohos",
        "version": {
            "code": 1,
            "name": "1.0"
        },
       "apiVersion": {
          "compatible": 3,
          "target": 3
       }
    },
    "deviceConfig": {
        "default": {
        }
    },
    "module": {
        "package":"com.ohos.FaceAuthTestServiceAuth.src",
        "name":"FaceAuthTestServiceAuth",
        "deviceType": [
            "tv",
            "car"
        ],
        "distro": {
            "deliveryWithInstall": true,
            "moduleName": "testability",
            "moduleType": "entry"
        },
        "abilities": [{
            "name": "FaceAuthTestServiceAbilityAuth",
            "icon": "$media:snowball",
            "label": "FaceAuthTestServiceAbilityAuth Ability",
            "launchType": "standard",
            "orientation": "unspecified",
            "type": "service",
            "srcLanguage":"c++",
            "visible": true
            }],
        "reqPermissions": [
            {
                "name": "ohos.permission.ACCESS_BIOMETRIC",
                "reason": "permreason_access_faceauth",
                "usedScene": 
                {
                    "ability": ["com.mycamera.Ability", "com.mycamera.AbilityBackground"],
                    "when": "always"
                }
            }
        ]
    }
}