{
    "description": "Config for print capi test cases",
    "driver": {
        "module-name": "ActsPrintCapiTest",
        "native-test-timeout": "300000",
        "native-test-device-path": "/data/local/tmp",
        "runtime-hint": "1s",
        "type": "CppTest"
    },
    "kits": [
        {
            "type": "PushKit",
            "push": [
                "ActsPrintCapiTest->/data/local/tmp/ActsPrintCapiTest"
            ],
            "post-push" : [
                "chmod -R 777 /data/local/tmp/*"
            ]
        }
    ]
}
