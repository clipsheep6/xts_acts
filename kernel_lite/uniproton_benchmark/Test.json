{
    "description": "Config for kernel_lite posix futex api test cases",
    "enviroment": [
        {
            "type": "device",
            "label": "ipcamera"
        }
    ],
    "kits": [
        {
            "type": "MoubtKit",
            "server": "NfsServer",
            "mount": [
               {
                     "source": "testcases/kernel",
                     "target": "test_root/kernel"
               }
            ]
        }
    ],
    "driver":{
        "type": "CppTestLite",
        "execute": "/test_root/kernel/benchmarkTest.bin"
    }
}