{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "60000",
        "shell-timeout": "60000",
        "testcase-timeout": 15000,
        "bundle-name": "com.example.uiabilitylifecycle",
        "module-name": "entry_test"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsUiAbilityEtsTest.hap",
                "commonTestAbilityApp.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "param set abilitymanagerservice.support.ecologicalrulemgrservice false"
            ],
            "teardown-command":[
                "param set abilitymanagerservice.support.ecologicalrulemgrservice true"
            ]
        }
    ]
}