{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.ohos.startabilitywantparamtest",
        "module-name": "entry_test",
        "shell-timeout": "600000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsStartAbilityWantParamTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
          "type": "ShellKit",
          "run-command": [
              "power-shell wakeup",
              "uinput -T -d 300 600 -m 300 600 300 100 -u 300 100",
              "uinput -T -m 300 2700 280 200",
              "power-shell setmode 602",
              "power-shell timeout -o 86400000"
          ]
      }  
    ]
}
