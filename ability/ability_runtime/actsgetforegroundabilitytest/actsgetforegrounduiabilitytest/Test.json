{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.getforegroundtest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "10000"
    },
    "kits": [
		{
            "test-file-name": [
                "ActsGetForegroundTest.hap",
                "ActsGetForegroundFirstRely.hap",
                "ActsGetForegroundModuleRely.hap",
                "ActsGetForegroundNoPermission.hap",
                "ActsGetForegroundNotSystem.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "uinput -T -m 300 2000 280 1950",
                "power-shell setmode 602",
                "setenforce 0"
            ]
        }
    ]
}