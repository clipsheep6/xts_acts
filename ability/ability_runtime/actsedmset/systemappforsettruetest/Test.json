{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.acts.myapplication",
        "module-name": "entry_test",
        "shell-timeout": "600000"
    },
    "kits": [
		{
            "test-file-name": [
                "systemappforsettrue.hap",
                "systemappforsettruetest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "uinput -T -m 300 2700 280 200",
                "setenforce 0"
            ]
        }
    ]
}