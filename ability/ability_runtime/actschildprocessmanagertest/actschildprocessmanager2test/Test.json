{
    "description": "Configuration for hjunit Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "60000",
        "bundle-name": "com.example.childprocess",
        "module-name": "entry",
        "shell-timeout": "30000",
        "testcase-timeout": 30000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsChildProcessManager2Test.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "param set persist.sys.multi_process_model false",
                "power-shell wakeup",
                "power-shell setmode 602",
                "hilog -Q pidoff",
                "hilog -b I -D 0xD001304",
                "hilog -G 5M"
            ]
        }
    ]
}