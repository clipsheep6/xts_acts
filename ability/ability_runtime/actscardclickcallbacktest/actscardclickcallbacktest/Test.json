{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.acts.cardclickcallback",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 100000
    },
    "kits": [
		{
            "test-file-name": [
                "ActsCardClickCallback.hap",
                "ActsCardClickCallbackNormal.hap",
                "ActsCardClickCallbackRely.hap",
                "ActsCardClickCallbackTest.hap",
                "ActsCardClickCallbackNoPermission.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "setenforce 0"
            ]
        }
    ]
}