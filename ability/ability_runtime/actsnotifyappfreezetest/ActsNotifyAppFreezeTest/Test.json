{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.appFreezeTest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "20000"
    },
    "kits": [
        {
            "type": "PushKit",
            "push": [
                "ActsAppFreezeRely.hap->/data/ActsAppFreezeRely.hap"
            ]
        },
        {
            "test-file-name": [
                "ActsAppFreezeTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "param set persist.sys.suspend_manager_enabled false",
                "reboot",
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "uinput -T -m 300 2000 280 1950",
                "power-shell setmode 602",
                "hilog -Q pidoff",
                "hilog -Q domainoff",
                "hilog -b D",
                "setenforce 0"
            ],
            "teardown-command": [
                "param set persist.sys.suspend_manager_enabled true",
                "power-shell setmode 601",
                "reboot"
	        ]
        }
    ]
}