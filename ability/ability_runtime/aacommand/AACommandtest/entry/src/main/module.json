{
  "module": {
    "name": "entry_test",
    "type": "entry",
    "srcEntrance": "./ets/Application/AbilityStage.ts",
    "description": "$string:entry_test_desc",
    "mainElement": "MainAbility",
    "deviceTypes": [
      "default",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "uiSyntax": "ets",
    "abilities": [
      {
        "name": "MainAbility",
        "srcEntrance": "./ets/MainAbility/MainAbility.ets",
        "description": "$string:MainAbility_desc",
        "icon": "$media:icon",
        "label": "$string:MainAbility_label",
        "visible": true,
        "launchType": "singleton",
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ]
      },
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/entryability/EntryAbility.ts",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:icon",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:startIcon",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.aacommandimplicitstarttest.home"
            ],
            "actions": [
              "action.aacommandimplicitstarttest.home"
            ]
          },
          {
            "entities": [
              "entity.aacommandimplicitstarttest.home"
            ],
            "actions": [
              "action.aacommandimplicitstarttest.home"
            ],
            "uris": [{
              "host": "uri.aacommandimplicitstarttest.com",
              "scheme": "https",
              "type": "uri-type"
            }]
          }
        ]
      },
      {
        "name": "UnexportedAbility",
        "srcEntry": "./ets/unexportedability/UnexportedAbility.ts",
        "description": "$string:UnexportedAbility_desc",
        "icon": "$media:icon",
        "label": "$string:UnexportedAbility_label",
        "startWindowIcon": "$media:startIcon",
        "exported": false,
        "skills": [
          {
            "entities": [
              "entity.system.unexportedability"
            ],
            "actions": [
              "action.system.unexportedability"
            ]
          }
        ]
      }
    ],
    "requestPermissions": [
      {
        "name":"ohos.permission.START_ABILITIES_FROM_BACKGROUND",
        "reason":"need use ohos.permission.START_ABILITIES_FROM_BACKGROUND"
      },
      {
        "name": "ohos.permission.START_INVISIBLE_ABILITY"
      }
    ]
  }
}