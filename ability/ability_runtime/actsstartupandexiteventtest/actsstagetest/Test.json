{
          "description": "Configuration for aceceshi Tests",
          "driver": {
              "type": "OHJSUnitTest",
              "test-timeout": "600000",
              "bundle-name": "com.example.actsstagetest",
              "module-name": "entry_test",
              "shell-timeout": "600000",
              "testcase-timeout": "20000"
          },
          "kits": [
                          {
                  "test-file-name": [
                      "ActsDataRely.hap",
                      "ActsFaRely.hap",
                      "ActsStageRely.hap",
                      "ActsStageTest.hap",
                      "ActsTerminateRely.hap",
                      "ActsTestAbilityRely.hap"
                  ],
                  "type": "AppInstallKit",
                  "cleanup-apps": true
              },
              {
                  "type": "ShellKit",
                  "run-command": [
                      "power-shell wakeup",
                      "power-shell setmode 602",
                      "hilog -Q pidoff",
                      "hilog -Q domainoff",
                      "hilog -b D",
                      "hilog -G 20M",
                      "setenforce 0"
                  ]
              }
          ]
      }