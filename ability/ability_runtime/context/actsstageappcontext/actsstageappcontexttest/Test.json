{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1200000",
        "bundle-name": "com.example.stageappcontexttest",
        "module-name": "entry_test",
        "shell-timeout": "1200000",
        "testcase-timeout": "30000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsStageAppContextTest.hap",
                "ClearUpAppDataHelpAssistEntry.hap",
                "ClearUpAppDataHelpAssistFeature.hap",
                "ClearUpAppDataHelpAssistSystem.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "param set persist.sys.suspend_manager_enabled false",
                "reboot",
                "power-shell wakeup",
                "power-shell setmode 602",
                "uinput -T -m 300 2700 280 200"
            ],
            "teardown-command": [
                "param set persist.sys.suspend_manager_enabled true",
                "power-shell setmode 601",
                "reboot"
            ]
        }
    ]
}