{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.stageappcontexttest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "15000"
    },
    "kits": [
		{
            "type": "ShellKit",
            "run-command": [
                "acm create -n user101 -t normal",
                "acm switch -i 101"
            ],
            "teardown-command": [
                "acm switch -i 100",
                "acm delete -i 101"
            ]
        },
		{
            "test-file-name": [
                "ActsStageAppContextTest.hap",
				"ActsAmsUsersKillProcessApp.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": false
        }
    ]
}

