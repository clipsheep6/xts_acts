{
  "app": {
    "bundleName": "com.example.fa8_context",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 8,
      "releaseType": "Release",
      "target": 8
    }
  },
  "deviceConfig": {},
  "module": {
