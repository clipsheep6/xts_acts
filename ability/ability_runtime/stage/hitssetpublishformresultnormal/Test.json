{
  "description": "Configuration for debuggable demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "1800000",
    "bundle-name": "ccom.hits.setPublishFormResultnormal",
    "module-name": "entry",
    "shell-timeout": "600000",
    "testcase-timeout": 60000
  },
  "kits": [
    {
      "test-file-name": [
        "HitssetpublishFormResultNormalTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "power-shell wakeup",
        "power-shell timeout -o 5000000",
        "power-shell setmode 602",
        "uinput -T -m 300 2700 280 200",
        "uinput -T -d 1070 1070 -u 1070 1070"
      ],
      "teardown-command": [
        "power-shell setmode 601"
      ]
    }
  ]
}