{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.acts.appexittest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 25000
    },
    "kits": [
		{
            
            "test-file-name": [
                "ActsAppExitRely.hap",
                "ActsAppExitSecondModule.hap",
                "ActsAppExitTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "param set persist.sys.abilityms.support.start_other_app true",
                "uinput -T -c 1080 1080",
                "uinput -T -d 654 2750 -m 654 2750 654 2284 -u 300 2284"
            ]
        }
    ]
}