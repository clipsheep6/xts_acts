{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.abilitymonitortest",
        "module-name": "entry",
        "shell-timeout": "600000"
    },
    "kits": [
        {
            "test-file-name": [
                "AbilityMonitorTest.hap"
            ],
            "type": "AppInstallKit",
            "run-command": [
                " hilog -Q pidoff",
                 "hilog -Q domainoff",
                 "hilog -b D",
                 "hilog -r"
             ],
            "cleanup-apps": true
        }
    ]
}

