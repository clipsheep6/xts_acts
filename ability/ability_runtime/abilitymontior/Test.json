{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.abilitymonitortest",
        "module-name": "entry",
        "shell-timeout": "600000",
        "testcase-timeout":"30000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAbilityMonitorTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
          "type": "ShellKit",
          "run-command": [
              "power-shell wakeup",
              "power-shell setmode 602",
              "uinput -T -m 300 2700 280 200"
          ]
      }
    ]
}

