                "icon": "$media:icon",
                "srcLanguage": "ets",
                "description": "$string:description_serviceability",
                "visible": true,
                "type": "service"
            }
        ],
        "js": [
            {
                "mode": {
                    "syntax": "ets",
                    "type": "pageAbility"
                },
                "pages": [
                    "pages/index",
                    "pages/second"
