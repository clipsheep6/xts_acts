{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.applicationrunningstatetest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "15000"
    },
    "kits": [
		{
            "test-file-name": [
                "ApplicationRunningStateTest.hap",
                "ApplicationRunningStateTestOneRely.hap",
                "ApplicationRunningStateTestTwoRely.hap",
                "ApplicationRunningStateTestThreeRely.hap",
                "ApplicationRunningStateTestFourRely.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "param set persist.sys.suspend_manager_enabled false",
                "reboot",
                "power-shell wakeup",
                "power-shell setmode 602",
                "uinput -T -m 300 3000 280 200",
                "setenforce 0"
            ]
        }
    ]
}