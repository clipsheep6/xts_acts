{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "EntryAbility",
    "deviceTypes": [
      "default",
      "tablet"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/entryability/EntryAbility.ts",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:icon",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ]
      },
      {
        "name": "SecondAbility",
        "srcEntry": "./ets/secondability/SecondAbility.ts",
        "description": "$string:EntryAbility1_desc",
        "icon": "$media:icon",
        "label": "$string:EntryAbility1_label",
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background",
        "visible":true
      }
    ],
    "extensionAbilities": [
      {
        "name": "ServiceExtensionAbility",
        "srcEntry": "./ets/serviceextensionability/ServiceExtensionAbility.ts",
        "description": "$string:ServiceExtensionAbility_desc",
        "icon": "$media:icon",
        "label": "$string:ServiceExtensionAbility_label",
        "visible": true,
        "type": "service"
      },
      {
        "name": "DataShareExtAbility",
        "srcEntrance": "./ets/dataShare/DataShare.ts",
        "icon": "$media:icon",
        "label": "$string:dataShare_label",
        "description": "$string:dataShare_desc",
        "type": "dataShare",
        "uri": "datashare://com.acts.switchuserextensionrely.DataShare",
        "exported": true,
        "visible": true
      },
      {
        "name": "SecondServiceExtensionAbility",
        "srcEntry": "./ets/secondserviceextensionability/SecondServiceExtensionAbility.ts",
        "description": "$string:SecondServiceExtensionAbility_desc",
        "icon": "$media:icon",
        "label": "$string:SecondServiceExtensionAbility_label",
        "visible": false,
        "type": "service"
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET"
      },
      {
        "name": "ohos.permission.READ_MEDIA"
      },
      {
        "name": "ohos.permission.WRITE_MEDIA"
      },
      {
        "name": "ohos.permission.MEDIA_LOCATION"
      },
      {
        "name": "ohos.permission.START_ABILITIES_FROM_BACKGROUND"
      }
    ]
  }
}