{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name":"com.example.inappcardfa",
        "package-name": "com.example.entry",
        "shell-timeout": "600000",
        "testcase-timeout": "15000"
    },
    "kits": [
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell setmode 602",
                "param set persist.ace.testmode.enabled 1",
                "hilog -Q pidoff",
                "hilog -Q domainoff",
                "hilog -b D"
            ]
        },
		{
            "test-file-name": [
                "ActsInAppCardFATest.hap",
                "ActsInAppCardLoadFARelyHap.hap",
                "ActsInAppCardLinkageFARelyHap.hap",
                "ActsInAppCardLinkageFAA.hap",
                "ActsInAppCardLinkageFAB.hap",
                "ActsInAppCardEventFARelyHap.hap",
                "ActsInAppCardActiveRefreshFARelyHap.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}

