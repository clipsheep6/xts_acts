{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.hitsautofillviewdatatest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "240000"
    },
    "kits": [
		{
            "test-file-name": [
                "TextAutoFillAbility.hap",
                "HitsAutoFillViewDataTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "param set persist.sys.suspend_manager_enabled false",
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602",
                "setenforce 0"
            ],
            "teardown-command": [
                "param set persist.sys.suspend_manager_enabled true"
            ]
        }
    ]
}