{
    "description": "Configuration for hjunit Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "shell-timeout": "30000",
        "bundle-name": "com.acts.multiinstancetest",
        "module-name": "entry_test",
        "testcase-timeout": 10000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsMultiInstanceTest.hap",
                "MultiInstanceHap1.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "uinput -T -m 300 2700 280 200"
            ]
        }
    ]
}