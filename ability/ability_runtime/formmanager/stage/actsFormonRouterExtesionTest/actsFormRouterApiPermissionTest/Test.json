{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "600000",
    "bundle-name": "com.example.formrouterapipermissiontest",
    "module-name": "entry_test",
    "shell-timeout": "600000",
    "testcase-timeout": 70000
  },
  "kits": [
    {
      "test-file-name": [
        "ActsFormRouterApiPermissionTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "uinput -T -d 300 600 -m 300 600 300 100 -u 300 100",
        "power-shell wakeup",
        "power-shell setmode 602"
      ]
    }
  ]
}