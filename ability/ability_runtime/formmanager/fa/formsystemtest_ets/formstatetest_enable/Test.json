{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "300000",
        "package": "com.ohos.st.formstateenabletest",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsFormStateEnableTest.hap",
                "FormSystemHostF.hap",
                "FormSystemHostG.hap",
                "FormSystemHostNoPerm.hap",
                "FormSupplyAppA.hap",
                "FormSupplyAppE.hap",
                "FormSupplyAppF.hap",
                "FormSupplyAppH.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}
