{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.observerappstatetest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "10000"
    },
    "kits": [
		{
            "test-file-name": [
                "ObserverAppStateOne.hap",
                "ObserverAppStateTwo.hap",
                "ObserverAppStateThree.hap",
                "ObserverAppStateFour.hap",
                "ObserverAppStateFive.hap",
                "ObserverAppStateSix.hap",
                "ObserverAppStateSeven.hap",
                "ObserverAppStateEight.hap",
                "ObserverAppStateNineOne.hap",
                "ObserverAppStateNineTwo.hap",
                "ObserverAppStateTenOne.hap",
                "ObserverAppStateTenTwo.hap",
                "ObserverAppStateEleven.hap",
                "ObserverAppStateTwelve.hap",
                "ObserverAppStateThirteen.hap",
                "ObserverAppStateFourteen.hap",
                "ObserverAppStateRely.hap",
                "ActsObserverAppStateTest.hap",
                "ActsObserverWindowModeRely.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "uinput -T -m 300 2000 280 1950",
                "power-shell setmode 602",
                "hilog -Q pidoff",
                "hilog -Q domainoff",
                "hilog -b D",
                "setenforce 0"
            ]
        }
    ]
}