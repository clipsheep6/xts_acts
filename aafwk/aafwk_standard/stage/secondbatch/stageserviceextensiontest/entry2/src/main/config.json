    "bundleName": "ohos.acts.aafwk.pldtest.myapplication",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.entry2",
    "name": ".MyApplication",
    "mainAbility": ".MainAbility",
