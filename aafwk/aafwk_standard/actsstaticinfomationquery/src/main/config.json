{
  "app": {
    "bundleName": "com.example.staticinfomationquery",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "removable": false
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.staticinfomationquery",
    "name": ".MyApplication",
    "mainAbility": ".MainAbility",
    "srcPath": "",
    "deviceType": [
      "phone"
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "name1",
          "value": "value1",
          "extra": "$string:customizeData_description"
        }
      ]
    },
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "metaData": {
          "customizeData": [
            {
              "name": "name1",
              "value": "value1",
              "extra": "$string:customizeData_description"
            }
          ]
        },
        "formEntity": 100,
        "minFormHeight": 200,
        "defaultFormHeight": 300,
        "minFormWidth": 400,
        "defaultFormWidth": 500,
        "orientation": "unspecified",
        "subType" : "asd",
        "visible": true,
        "srcPath": "MainAbility",
        "name": ".MainAbility",
        "srcLanguage": "ets",
        "icon": "$media:icon",
        "description": "$string:description_mainability",
        "formsEnabled": false,
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "srcPath": "DataAbility",
        "name": ".DataAbility",
        "icon": "$media:icon",
        "srcLanguage": "ets",
        "description": "$string:description_dataability",
        "type": "data",
        "uri": "dataability://com.example.staticinfomationquery.DataAbility",
        "writePermission": "writePermission",
        "readPermission": "readPermission"
      },
      {
        "backgroundModes": [
          "dataTransfer",
          "audioPlayback",
          "audioRecording",
          "location",
          "bluetoothInteraction",
          "multiDeviceConnection",
          "wifiInteraction",
          "voip",
          "taskKeeping"
        ],
        "srcPath": "ServiceAbility",
        "name": ".ServiceAbility",
        "icon": "$media:icon",
        "srcLanguage": "ets",
        "description": "$string:description_serviceability",
        "type": "service"
      }
    ],
    "js": [
      {
        "mode": {
          "syntax": "ets",
          "type": "pageAbility"
        },
        "pages": [
          "pages/index",
          "pages/second"
        ],
        "name": ".MainAbility",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ],
    "defPermissions": [
      {
        "name": "com.example.staticinfomationquery.DataAbilityShellProvider.PROVIDER"
      }
    ]
  }
}