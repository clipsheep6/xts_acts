{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "480000",
        "package": "com.example.formfuzztest",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "FormFuzzTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "PushKit",
            "push": [
                "FormFuzzTest.hap->/data/FormFuzzTest.hap"
            ]
            
        }    
    ]
}
