{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "240000",
        "package": "com.example.forminfo03",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "FormInfo.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "PushKit",
            "push": [
                "FormInfo.hap->/data/FormInfo.hap"
            ]
            
        }    
    ]
}
