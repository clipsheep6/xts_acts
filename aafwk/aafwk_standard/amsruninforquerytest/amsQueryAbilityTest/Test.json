{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "120000",
        "package": "com.example.amsabilityquerytest",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "actsamsabilityquerytest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "remount",
                "mkdir /data/test"
            ]
        },
        {
            "type": "PushKit",
            "push": [
                "actsamsabilityquerytest.hap->/data/test/actsamsabilityquerytest.hap",
                "querytestone.hap->/data/test/querytestone.hap",
                "querytesttwo.hap->/data/test/querytesttwo.hap",
                "querytestfour.hap->/data/test/querytestfour.hap",
                "querytestfive.hap->/data/test/querytestfive.hap",
                "querytestfeature.hap->/data/test/querytestfeature.hap",
                "querytestsix.hap->/data/test/querytestsix.hap",
                "querytestmulti.hap->/data/test/querytestmulti.hap",
                "querytestmultifeature.hap->/data/test/querytestmultifeature.hap"
            ]
        },
        {
            "type": "ShellKit",
            "run-command": [
                "chmod 644 /data/test/*.hap"
            ]
        }
    ]
}
