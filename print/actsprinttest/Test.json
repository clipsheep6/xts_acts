{
    "description": "Configuration for ActsPrintTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "300000",
		"bundle-name": "com.open.harmony.printxts",
        "package-name": "com.open.harmony.printxts",
        "shell-timeout": "600000",
		"testcase-timeout": "10000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsPrintTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "setenforce 0"
            ]
        }
    ]
}
