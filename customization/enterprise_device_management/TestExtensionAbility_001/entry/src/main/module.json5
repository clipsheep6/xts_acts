      },
      {
        "name": "ohos.permission.ENTERPRISE_RESTRICT_POLICY"
      },
      {
        "name": "ohos.permission.ENTERPRISE_SET_BROWSER_POLICY"
      },
      {
        "name": "ohos.permission.ENTERPRISE_MANAGE_SYSTEM"
      },
      {
        "name": "ohos.permission.ENTERPRISE_MANAGE_NETWORK"
