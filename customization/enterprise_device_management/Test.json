{
    "description": "Configuration for enterprise device management Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "300000",
        "package": "ohos.edm.test",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "type": "ShellKit",
            "run-command": [
                "acm create -n edmuser -t normal"
            ],
            "teardown-command": [
                "acm delete -i 101"
            ]
        },
        {
            "test-file-name": [
                "ActsEdmJsTest.hap",
                "edmJstest1.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}
