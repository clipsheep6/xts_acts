{
    "description": "Config for NativeXComponent test cases",
    "driver": {
        "module-name": "ActsNativeXComponentTest",
        "native-test-timeout": "120000",
        "native-test-device-path": "/data/local/tmp",
        "runtime-hint": "1s",
        "type": "CppTest"
    },
    "kits": [
        {
            "post-push" : [
                "chmod -R 777 /data/local/tmp/*"
            ],
            "push": [
                "ActsNativeXComponentTest->/data/local/tmp/ActsNativeXComponentTest"
            ],
            "type": "PushKit"
        },
        {
            "type": "ShellKit",
            "run-command": [
                "remount",
                "mkdir /data/test"
            ]
        }
    ]
}