{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "270000",
        "shell-timeout": "60000",
        "bundle-name": "ohos.acts.hiviewdfx.hilog.function",
        "package-name": "ohos.acts.hiviewdfx.hilog.function_test"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsHilogJsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {   
            "type":"ShellKit",
             "run-command":[
                "hilog -Q pidoff",
                "hilog -Q domainoff"
            ],
            "teardown-command":[
                "hilog -Q pidon",
                "hilog -Q domainon"
            ]
        }
    ]
}