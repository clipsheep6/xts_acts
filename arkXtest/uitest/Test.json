{
    "description": "Configuration for uitest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1800000",
        "bundle-name": "com.uitest.test",
        "package-name": "com.uitest.test",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "$module.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}