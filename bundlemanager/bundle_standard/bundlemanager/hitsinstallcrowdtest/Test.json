{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.example.installcrowdtest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 70000
    },
    "kits": [
        {
            "test-file-name": [
                "HitsInstallCrowdTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
	{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        },
        {
            "type": "PushKit",
            "push": [
                "bmsInstallCrowd0200.hap->/data/app/el2/100/base/com.example.installcrowdtest/files/",
                "bmsInstallCrowd02001.hap->/data/app/el2/100/base/com.example.installcrowdtest/files/"
            ]
        }
    ]
}

