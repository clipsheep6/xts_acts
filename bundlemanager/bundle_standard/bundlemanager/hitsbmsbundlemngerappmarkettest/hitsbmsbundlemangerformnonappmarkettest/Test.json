{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "600000",
    "bundle-name": "com.example.bundleMangerFormNonAppMarketTest",
    "module-name": "entry_test",
    "shell-timeout": "600000",
    "testcase-timeout": 70000
  },
  "kits": [
    {
      "test-file-name": [
        "hitsbmsbundlemangerformnonappmarkettest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "wait 10",
        "mount -o rw,remount /",
        "rm -rf system/app/AppGallery",
        "rm -rf data/app/el1/bundle/public/com.huawei.amsapp.appgallery/",
        "sync",
        "reboot",
        "power-shell wakeup",
        "uinput -T -d 300 600 -m 300 600 300 100 -u 300 100",
        "power-shell setmode 602"
      ]
    }
  ]
}