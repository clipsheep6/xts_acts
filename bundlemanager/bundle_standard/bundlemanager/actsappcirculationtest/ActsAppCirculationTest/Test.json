                "ActsAppCirculationSameVersionFeatureRely.hap->/data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/haptest/ActsAppCirculationSameVersionFeatureRely.hap"
            ]
        }
    ]
