                "ActsAppCirculationUpdateWithoutAppIdentifierDiffModuleAppIdRely.hap->/data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationUpdateWithoutAppIdentifierDiffModuleAppIdRely/ActsAppCirculationUpdateWithoutAppIdentifierDiffModuleAppIdRely.hap",
                "ActsAppCirculationUpdateWithoutAppIdentifierRely.hap->/data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationUpdateWithoutAppIdentifierRely/ActsAppCirculationUpdateWithoutAppIdentifierRely.hap",
