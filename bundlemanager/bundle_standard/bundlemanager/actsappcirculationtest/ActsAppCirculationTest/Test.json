{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.actsappcirculationtest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "50000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAppCirculationTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationBaseRely",
                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationUpdateRely",
                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationDiffModuleUpdateRely",
                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationDiffIdUpdateRely",
                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationDiffModuleIdUpdateRely",
                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationDiffAppIdUpdateRely",
                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationDiffAppIdModuleUpdateRely",
