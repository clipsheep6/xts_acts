                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationUpdateWithoutAppIdentifierRely",
                "mkdir -p /data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/haptest",
                "power-shell wakeup",
                "power-shell setmode 602",
                "hilog -Q pidoff",
                "hilog -Q domainoff",
                "hilog -b D",
                "setenforce 0"
            ]
        },
        {
            "type": "PushKit",
            "push": [
                "ActsAppCirculationBaseRely.hap->/data/app/el2/100/base/com.example.actsappcirculationtest/haps/entry_test/ActsAppCirculationBaseRely/ActsAppCirculationBaseRely.hap",
