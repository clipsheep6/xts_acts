        "extensionAbilities": [
            {
                "name": "FormAbility",
                "srcEntrance": "./ets/FormAbility/FormAbility.ts",
                "label": "$string:form_FormAbility_label",
                "description": "$string:form_FormAbility_desc",
                "type": "form",
                "skills": [
                    {
                        "actions": ["actionZ"]
