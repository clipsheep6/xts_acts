{
  "module": {
    "name": "phone",
    "type": "entry",
    "srcEntrance": "./ets/Application/AbilityStage.ts",
    "description": "$string:entry_desc",
    "mainElement": "MainAbility",
    "deviceTypes": [
      "default",
      "phone",
      "tablet"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "uiSyntax": "ets",
    "abilities": [
      {
        "name": "com.example.abilitylisttest.MainAbility",
        "srcEntrance": "./ets/MainAbility/MainAbility.ts",
        "description": "$string:MainAbility_desc",
        "icon": "$media:icon",
        "label": "$string:MainAbility_label",
        "visible": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ]
      }
    ],
    "extensionAbilities": [
      {
        "srcEntrance": "./ets/ServiceExtAbility/ServiceAudibleAbility.ts",
        "name": "ServiceAudibleAbility",
        "icon": "$media:icon",
        "description": "$string:description_serviceability",
        "type": "accessibility",
        "visible": true,
        "metadata": [
          {
            "name": "ohos.accessibleability",
            "resource": "$profile:accessibility_audible_config"
          }
        ]
      },
      {
        "srcEntrance": "./ets/ServiceExtAbility/ServiceGenericAbility.ts",
        "name": "ServiceGenericAbility",
        "icon": "$media:icon",
        "description": "$string:description_serviceability",
        "type": "accessibility",
        "visible": true,
        "metadata": [
          {
            "name": "ohos.accessibleability",
            "resource": "$profile:accessibility_generic_config"
          }
        ]
      },
      {
        "srcEntrance": "./ets/ServiceExtAbility/ServiceHapticAbility.ts",
        "name": "ServiceHapticAbility",
        "icon": "$media:icon",
        "description": "$string:description_serviceability",
        "type": "accessibility",
        "visible": true,
        "metadata": [
          {
            "name": "ohos.accessibleability",
            "resource": "$profile:accessibility_haptic_config"
          }
        ]
      },
      {
        "srcEntrance": "./ets/ServiceExtAbility/ServiceSpokenAbility.ts",
        "name": "ServiceSpokenAbility",
        "icon": "$media:icon",
        "description": "$string:description_serviceability",
        "type": "accessibility",
        "visible": true,
        "metadata": [
          {
            "name": "ohos.accessibleability",
            "resource": "$profile:accessibility_spoken_config"
          }
        ]
      },
      {
        "srcEntrance": "./ets/ServiceExtAbility/ServiceVisualAbility.ts",
        "name": "ServiceVisualAbility",
        "icon": "$media:icon",
        "description": "$string:description_serviceability",
        "type": "accessibility",
        "visible": true,
        "metadata": [
          {
            "name": "ohos.accessibleability",
            "resource": "$profile:accessibility_visual_config"
          }
        ]
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.SYSTEM_FLOAT_WINDOW",
        "reason": "need use ohos.permission.SYSTEM_FLOAT_WINDOW"
      },
      {
        "name": "ohos.permission.CAPTURE_SCREEN",
        "reason": "need use ohos.permission.SYSTEM_FLOAT_WINDOW"
      },
      {
        "name": "ohos.permission.WRITE_ACCESSIBILITY_CONFIG",
        "reason": "need use ohos.permission.WRITE_ACCESSIBILITY_CONFIG"
      }
    ]
  }
}
