{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "380000",
        "bundle-name": "com.example.actioninitiationtest",
        "module-name": "phone",
        "shell-timeout": "600000",
        "testcase-timeout": 70000
    },
    "kits": [
        {
        "test-file-name": [
            "ActsActionInitiationTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
        },        
        {
        "type": "ShellKit",
        "teardown-command":[
            "bm uninstall -n com.example.actioninitiationtest"
        ]
        }
    ]
}
