{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "280000",
        "bundle-name": "com.example.checkabilityisopen",
        "module-name": "phone",
        "shell-timeout": "600000",
        "testcase-timeout": 70000
    },
    "kits": [
        {
        "test-file-name": [
            "ActsCheckAbilityTest.hap",
            "accessibilityBaseAbility.hap",
            "accessibilityCheckAbility.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
        },        
        {
        "type": "ShellKit",
        "run-command": [
            "param set persist.ace.testmode.enabled 1"
        ],
        "teardown-command":[
            "bm uninstall -n com.example.checkabilityisopen",
            "bm uninstall -n com.example.accessiblebaseability.hmservice",
            "bm uninstall -n com.example.accessiblecheckability.hmservice"
        ]
        }
    ]
}
