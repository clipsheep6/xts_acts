{
    "description": "Configuration for bluetooth api Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "shell-timeout": "180000",
        "bundle-name": "ohos.acts.communication.wifi.wifiSta",
        "module-name": "entry_test"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsEtsWifiTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "svc bluetooth enable",
                "power-shell setmode 602"
            ]
        }
    ]
}